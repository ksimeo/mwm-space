  <modelVersion>4.0.0</modelVersion>
  <groupId>com.ksimeo.arsu.admin</groupId>
  <artifactId>mwm-space-admin</artifactId>
  <packaging>war</packaging>
  <version>1.0</version>
  <name>MWM-Space_Admin</name>
